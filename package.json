{
  "name": "auth-ui",
  "version": "0.0.1",
  "type": "module",
  "description": "Web components based UI elements for common auth actions.",
  "zshy": "./src/index.ts",
  "scripts": {
    "build": "tsc && zshy",
    "lint": "biome lint",
    "check": "biome check",
    "format": "biome format --write",
    "check:write": "biome check --write",
    "release": "pnpm build && changeset publish"
  },
  "license": "MIT",
  "dependencies": {
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@changesets/cli": "^2.29.5",
    "typescript": "^5.8.3",
    "zshy": "^0.2.5"
  },
  "files": [
    "**/*.js",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.d.ts",
    "**/*.d.mts",
    "**/*.d.cts"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
